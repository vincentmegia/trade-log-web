{
  "name": "tradelog",
  "version": "1.0.0",
  "description": "trade diary tool for PSE",
  "scripts": {
    "start": "tsc && npm install && npm run build && \"dotnet run\" ",
    "startWebpackDevServer": "webpack-dev-server --inline --progress --port 8081",
    "build": "SET NODE_ENV=development && webpack -d --color && dotnet run",
    "buildProduction": "SET NODE_ENV=production && webpack -d --color",
    "test": "karma start --auto-watch",
    "test-once": "tsc && karma start karma.conf.js --single-run"
  },
